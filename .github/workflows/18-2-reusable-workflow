name: 18.2-Reusbale-workflow
on:
  workflow_dispatch:
jobs:
  deploy: 
    uses: ./.github/workflows/18-1-resusable-workflows.yaml
    with: 
      target-directory: dummy-dir
  prints-outputs: 
     runs-on: ubuntu-latest
     needs: deploy
     steps: 
      - name: prints outputs
        run: |
          echo "Build status: ${{needs.deploy.outputs.build-status}}"
          echo "URL: ${{needs.deploy.outputs.url}}"







          
      
